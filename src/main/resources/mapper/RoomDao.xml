<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="stu.software.chatroom.room.dao.RoomDao">

    <!--    上传用户头像-->
    <update id="uploadAvatar">
        update t_user set u_avatar = #{url} where u_id = #{u_id}
    </update>

</mapper>